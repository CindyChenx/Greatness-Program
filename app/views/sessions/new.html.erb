<%= form_tag("/login") do %>

    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br>
    <%= submit_tag "Login" %>
    
<% end %>

<div class="button1">
    <%= link_to('Log in with Github!', '/auth/github') %>
</div>